<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.communitycleanup">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission-sdk-23 android:name="android.permission.CAMERA"/>
    <uses-permission-sdk-23 android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivities.CheckOut.NoCheckOut"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".HomeActivities.CheckOut.CheckOutSuccess"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".HomeActivities.CheckIn.CheckInSuccess"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".HomeActivities.CheckIn.NoEventsActivity"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".HomeActivities.AboutActivity"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".HomeActivities.EventsActivity"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".HomeScreen"/>
        <activity android:name=".HomeActivities.DrugEvidenceActivity"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivities.CheckIn.CheckInActivity"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".HomeActivities.CheckOut.CheckOutActivity"
            android:parentActivityName=".HomeScreen"/>
        <activity android:name=".HomeActivities.MyAccount"
            android:parentActivityName=".HomeScreen"/>
    </application>

</manifest>